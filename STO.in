&CONTROL
                       title = 'YBCO_STO_PBE_from_GBRV' ,
		       restart_mode = 'from_scratch' ,
                 calculation = 'vc-relax' ,
                  wf_collect = .true. ,
                      outdir = '../outdir/' ,
                      wfcdir = '../temp/' ,
                  pseudo_dir = '../pbe/' ,
                      prefix = 'test_1_2' ,
                     disk_io = 'default' ,
                   verbosity = 'default' ,
                      iprint = 10 ,
               etot_conv_thr = 1.0d-6 ,
               forc_conv_thr = 1.0d-6 ,
                       nstep = 100 ,
                     tstress = .false. ,
                     tprnfor = .false. ,
 /
 &SYSTEM
                       ibrav = 8,
                           A = 3.944 ,
                           B = 3.944 ,
                           C = 78.144 , !63.144 + 15
                       cosAB = 0 ,
                       cosAC = 0 ,
                       cosBC = 0 ,
                         nat = 21,
                        ntyp = 6,
                     ecutwfc = 40 ,
                     ecutrho = 200 ,
		     !nbnd = 105 ,
		     occupations = 'smearing' ,
		     degauss = 0.01 ,
