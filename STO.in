	     mixing_mode = 'local-TF' ,
	     mixing_ndim = 8 ,
	     conv_thr = 2e-5 ,
 /
 &IONS
                ion_dynamics = 'damp' ,
               ion_positions = 'default' ,
 /
 &CELL
               cell_dynamics = 'damp-pr' ,
                 cell_factor = 1.5 ,
                 cell_dofree = 'z' ,
